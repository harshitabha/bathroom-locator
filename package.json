{
  "name": "bathroom-locator",
  "description": "CSE 115A Bathroom Locator Web App",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "install-frontend": "cd frontend && npm install",
    "install-backend": "cd backend && npm install",
    "lint-frontend": "cd frontend && npm run lint",
    "lint-backend": "cd backend && npm run lint",
    "lint-frontend-fix": "cd frontend && npm run lint-fix",
    "lint-backend-fix": "cd backend && npm run lint-fix",
    "lint": "(npm run lint-frontend) && (npm run lint-backend)",
    "lint-fix": "(npm run lint-frontend-fix) && (npm run lint-backend-fix)",
    "start-backend": "cd backend && npm start",
    "start-frontend": "cd frontend && npm run dev",
    "test-backend": "cd backend && npm test",
    "test-frontend": "cd frontend && npm test",
    "test": "(npm run test-backend) && (npm run test-frontend)",
    "prepare": "husky"
  },
  "devDependencies": {
    "husky": "^9.1.7",
    "vitest": "^3.2.4"
  }
}